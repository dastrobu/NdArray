{"metadata":{"extendedModule":"NdArray","roleHeading":"Instance Method","symbolKind":"method","title":"luInPlace()","role":"symbol","externalID":"s:7NdArray6MatrixCAASfRszlE9luInPlaceAA6VectorCys5Int32VGyKF","conformance":{"availabilityPrefix":[{"text":"Available when","type":"text"}],"constraints":[{"type":"codeVoice","code":"T"},{"text":" is ","type":"text"},{"type":"codeVoice","code":"Float"},{"text":".","type":"text"}],"conformancePrefix":[{"text":"Conforms when","type":"text"}]},"modules":[{"name":"NdArray"}],"fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"luInPlace"},{"kind":"text","text":"() "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"preciseIdentifier":"s:7NdArray6VectorC","kind":"typeIdentifier","text":"Vector"},{"kind":"text","text":"<"},{"preciseIdentifier":"c:@T@__CLPK_integer","kind":"typeIdentifier","text":"__CLPK_integer"},{"kind":"text","text":">"}]},"hierarchy":{"paths":[["doc:\/\/NdArray\/documentation\/NdArray","doc:\/\/NdArray\/documentation\/NdArray\/Matrix"]]},"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"text":"func","kind":"keyword"},{"text":" ","kind":"text"},{"text":"luInPlace","kind":"identifier"},{"text":"() ","kind":"text"},{"text":"throws","kind":"keyword"},{"text":" -> ","kind":"text"},{"identifier":"doc:\/\/NdArray\/documentation\/NdArray\/Vector","preciseIdentifier":"s:7NdArray6VectorC","text":"Vector","kind":"typeIdentifier"},{"text":"<","kind":"text"},{"preciseIdentifier":"c:@T@__CLPK_integer","text":"__CLPK_integer","kind":"typeIdentifier"},{"text":">","kind":"text"}],"platforms":["macOS"],"languages":["swift"]}]},{"kind":"content","content":[{"type":"heading","level":2,"anchor":"discussion","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Factorization is computed by LAPACKs SGETRF method, which computes an LU factorization of a general"},{"type":"text","text":" "},{"type":"text","text":"
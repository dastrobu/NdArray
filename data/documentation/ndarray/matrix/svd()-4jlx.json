real and non-negative, and"},{"type":"text","text":" "},{"type":"text","text":"are returned in descending order.  The first min(m,n) columns of"},{"type":"text","text":" "},{"type":"text","text":"U and V are the left and right singular vectors of A."}],"type":"paragraph"}]}],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/NdArray\/documentation\/NdArray\/Matrix\/svd()-4jlx"},"references":{"doc://NdArray/documentation/NdArray/Vector":{"title":"Vector","role":"symbol","navigatorTitle":[{"text":"Vector","kind":"identifier"}],"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Vector","kind":"identifier"}],"kind":"symbol","type":"topic","identifier":"doc:\/\/NdArray\/documentation\/NdArray\/Vector","abstract":[{"type":"text","text":"Special NdArray subtype for one dimensional data."}],"url":"\/documentation\/ndarray\/vector"},"doc://NdArray/documentation/NdArray":{"role":"collection","abstract":[],"identifier":"doc:\/\/NdArray\/documentation\/NdArray","title":"NdArray","kind":"symbol","type":"topic","url":"\/documentation\/ndarray"},"doc://NdArray/documentation/NdArray/Matrix":{"fragments":[{"text":"class","kind":"keyword"},{"text":" ","kind":"text"},{"text":"Matrix","kind":"identifier"}],"title":"Matrix","navigatorTitle":[{"text":"Matrix","kind":"identifier"}],"abstract":[{"text":"Special NdArray subtype for two dimensional data.","type":"text"}],"url":"\/documentation\/ndarray\/matrix","identifier":"doc:\/\/NdArray\/documentation\/NdArray\/Matrix","role":"symbol","type":"topic","kind":"symbol"},"doc://NdArray/documentation/NdArray/Matrix/svd()-4jlx":{"title":"svd()","type":"topic","url":"\/documentation\/ndarray\/matrix\/svd()-4jlx","role":"symbol","identifier":"doc:\/\/NdArray\/documentation\/NdArray\/Matrix\/svd()-4jlx","abstract":[{"text":"Compute the SVD","type":"text"}],"conformance":{"availabilityPrefix":[{"type":"text","text":"Available when"}],"conformancePrefix":[{"type":"text","text":"Conforms when"}],"constraints":[{"